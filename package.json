{
  "name": "hipo-frontend-ts-boilerplate",
  "version": "0.0.0",
  "description": "Hipo Frontend TypeScript Boilerplate",
  "private": true,
  "scripts": {
    "build:release": "webpack --config webpack.release.config.js  --progress --colors",
    "build:dev": "webpack --config webpack.dev.config.js --progress --colors",
    "build:dev:live": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js --progress --colors --watch",
    "start": "npm run build:dev:live",
    "type-check": "echo \"type-checking...\" && tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "eslint": "./node_modules/.bin/eslint --ext .jsx,.js,.tsx,.ts src/",
    "eslint:fix": "./node_modules/.bin/eslint --fix --ext .jsx,.js,.tsx,.ts src/",
    "stylelint": "echo \"stylelint-checking...\" && ./node_modules/.bin/stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettify": "./node_modules/.bin/prettier --config ./.prettierrc.js --write 'src/**/*.{ts,tsx}'",
    "lint": "npm run stylelint; npm run eslint",
    "test": "./node_modules/.bin/jest",
    "test:watch": "npm t -- --watch",
    "changelog": "./node_modules/.bin/conventional-changelog --config ./node_modules/hipo-web-conventional-changelog/index.js --context ./changelog-context-variables.json -o CHANGELOG.md -r 0",
    "checkPackageUpdates": "npx npm-check-updates"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hipo/hipo-frontend-ts-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Hipo/hipo-frontend-ts-boilerplate/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@hipo/eslint-config-base": "latest",
    "@hipo/eslint-config-react": "latest",
    "@hipo/stylelint-config-base": "latest",
    "@svgr/webpack": "5.3.1",
    "@types/classnames": "2.2.10",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.2.1",
    "@types/pluralize": "0.0.29",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "@types/react-modal": "3.10.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.4",
    "@types/redux": "3.6.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "autoprefixer": "9.7.6",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "conventional-changelog-cli": "2.0.31",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.5.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.3",
    "hipo-web-conventional-changelog": "github:Hipo/hipo-web-conventional-changelog",
    "html-webpack-plugin": "4.2.0",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "lint-staged": "10.1.7",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "postcss-inline-svg": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "prettier": "2.0.5",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.4",
    "stylelint": "13.3.3",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.17.1",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2",
    "webpack-s3-plugin": "1.0.3"
  },
  "dependencies": {
    "@hipo/hipo-exceptions-js": "1.0.4",
    "@hot-loader/react-dom": "16.13.0",
    "@sentry/browser": "5.15.5",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "pluralize": "8.0.0",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.20",
    "react-modal": "3.11.2",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-saga": "1.1.3",
    "regenerator-runtime": "0.13.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify && lint-staged && npm run type-check",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint",
      "git add"
    ],
    "*.{css,scss}": [
      "npm run stylelint",
      "git add"
    ]
  }
}
